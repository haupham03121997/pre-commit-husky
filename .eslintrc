{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
              "arrowParens": "always",
              "semi": false,
              "trailingComma": "none",
              "tabWidth": 2,
              "endOfLine": "auto",
              "useTabs": false,
              "singleQuote": true,
              "printWidth": 120,
              "jsxSingleQuote": true,
              "no-console": "warn"
            }
          ]
    }
}
